  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "clear-cache": "./clearCache.sh",
    "expo:fix-modules": "npx expo install --fix",
    "expo:add": "npx expo install",
    "prebuild": "npx expo prebuild --clean --yarn",
    "android": "npx expo run android",
    "ios": "npx expo run ios -d",
    "run:ios-no-cache": "npx expo run ios --no-build-cache",
    "run:tunnel": "npx expo start --tunnel",
    "run:prod": "npx expo start --no-dev --minify",
    "sys-log:android": "npx react-native log-android",
    "sys-log:ios": "npx react-native log-ios",
    "config:check": "npx expo config --type introspect",
    "config:show": "npx expo config",
    "android:dark": "adb shell \"cmd uimode night yes\"",
    "android:light": "adb shell \"cmd uimode night no\"",
    "adb-logcat": "adb logcat",
    "fix-code": "prettier --write src/"
